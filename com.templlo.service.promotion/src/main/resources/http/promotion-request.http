  "endDate": "2025-01-10",
  "couponType": "DISCOUNT",
  "maleCoupon": 100,
  "femaleCoupon": 100,
  "totalCoupon": 200
}

> {%
    // 응답에서 promotionId 추출하여 변수 저장
